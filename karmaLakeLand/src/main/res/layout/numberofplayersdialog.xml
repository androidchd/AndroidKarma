<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="200dp"
    android:layout_height="150dp"
   
    android:orientation="horizontal" 
    android:layout_marginLeft="10dp"
    android:layout_marginRight="10dp"
    android:weightSum="100">
   <LinearLayout
       android:background="@color/dialog_background"
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:orientation="vertical"
       android:weightSum="100">
       <LinearLayout 
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:orientation="vertical"
       android:layout_weight="5">
       <Button
        android:layout_marginTop="5dp"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:paddingBottom="10dp"
        android:background="#3061e5"
        android:textSize="14sp"
        android:textColor="#FFFFFF"
        android:layout_gravity="center"
        android:textStyle="bold"
        android:text="Rate Card" />
       </LinearLayout>
   
    <TableLayout
        android:layout_width="match_parent"
        android:id="@+id/tablel1"
        android:layout_height="match_parent"
        android:layout_weight="40"
        android:stretchColumns="*">
        
        

  
   <TableRow
            android:id="@+id/tablewrow8"                 
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
             android:weightSum="3"
             android:background="#FFFFFF"

          >
       <TextView
              android:layout_weight="1"
              android:background="@drawable/pop_up_button"
              android:text="Day"
              android:layout_height="25dp"
              android:textColor="#3061e5"
              android:paddingTop="4dp"
              android:gravity="center_horizontal"
              android:textSize="12sp" />

          <TextView
              android:layout_weight="1"
              android:background="@drawable/pop_up_button"
              android:text="Bucket Fee"
              android:layout_height="25dp"
              android:textColor="#3061e5"
              android:paddingTop="4dp"
              android:gravity="center_horizontal"
              android:textSize="12sp" />

        

         
   </TableRow>
   <TableRow
            android:id="@+id/tablewrow9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
             android:weightSum="3"
             android:background="#FFFFFF"

          >

          <TextView
              android:layout_weight="1"
              android:background="@drawable/pop_up_button"
              android:gravity="center_horizontal"
              android:text="Mon-Fri"
              android:layout_height="25dp"
              android:paddingTop="4dp"
              android:textSize="12sp" />

          <TextView
              android:layout_weight="1"
              android:id="@+id/bucketfee1"
              android:background="@drawable/pop_up_button"
              android:gravity="center_horizontal"
              android:text="Rs.900"
              android:layout_height="25dp"
              android:paddingTop="4dp"
              android:textSize="12sp" />

         
   </TableRow>
   <TableRow
            android:id="@+id/tablewrow10"                 
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
             android:weightSum="3"
             android:background="#FFFFFF"
           
          >
          <TextView
              android:layout_weight="1"
              android:background="@drawable/pop_up_button"
              android:text="Sat-Sun"
              android:layout_height="25dp"
              android:paddingTop="4dp"
              android:gravity="center_horizontal"
              android:textSize="12sp" />

          <TextView
              android:layout_weight="1"
              android:background="@drawable/pop_up_button"
              android:text="Rs.1300"
              android:id="@+id/bucketfee2"
              android:layout_height="25dp"
              android:paddingTop="4dp"
              android:gravity="center_horizontal"
              android:textSize="12sp" />

          

          
           </TableRow>
  <TableRow
            android:id="@+id/tablewrow11"                 
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#FFFFFF"
            android:weightSum="1"
           
          >
         
              
          </TableRow>
    </TableLayout>
    <LinearLayout 
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:orientation="horizontal"
      android:layout_marginTop="2dp"
        android:weightSum="2"
       android:layout_weight="40">
  
        
   
  
 <Button 
        android:id="@+id/ok"
        
       android:layout_weight="1"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginLeft="20dp"
        android:background="@drawable/buttonstyle4"
        android:textSize="12sp"
        android:layout_marginTop="10dp"
        android:textColor="#FFFFFF"
        android:gravity="center"
     
        android:text="OK" />
  <Button 
        android:id="@+id/cancel"
        android:layout_marginRight="20dp"
       android:layout_weight="1"
       android:layout_marginLeft="5dp"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:background="#B0B0B2"
        android:textSize="12sp"
        android:gravity="center"
        android:textColor="#FFFFFF"
       android:layout_marginTop="10dp"
        android:text="Cancel" />
</LinearLayout>
</LinearLayout>
</LinearLayout>